.goods_list_wrapper {
  height: 100%;
}
.goods_list_wrapper .comprehensive_item {
  display: flex;
  padding: 20rpx 20rpx;
}
.goods_list_wrapper .comprehensive_item:not(:last-child) {
  border-bottom: 1rpx solid #ddd;
}
.goods_list_wrapper .comprehensive_item .goods_img_wrapper {
  flex: 2;
  text-align: center;
}
.goods_list_wrapper .comprehensive_item .goods_img_wrapper image {
  width: 70%;
  background: #ddd;
}
.goods_list_wrapper .comprehensive_item .goods_info_wrapper {
  flex: 5;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
}
.goods_list_wrapper .comprehensive_item .goods_info_wrapper .goods_name {
  display: -webkit-box;
  overflow: hidden;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  text-overflow: ellipsis;
}
.goods_list_wrapper .comprehensive_item .goods_info_wrapper .goods_price {
  color: var(--themeColor);
  font-weight: 600;
}
.goods_list_wrapper .no_data {
  width: 100%;
  height: calc(100% - 73px);
  background: #f5f5f3;
}
.goods_list_wrapper .no_data image {
  width: 70%;
}

