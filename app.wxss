page,
view,
text,
swiper,
swiper-item,
image,
navigator {
  padding: 0;
  margin: 0;
  box-sizing: border-box;
}

page {
  --themeColor: #eb4450;
  --BG: #ededec;
  --color999: #999;
  --color666: #666;
  --color333: #333;
  --colorccc: #ccc;
  font-size: 28rpx;
  --fontS20: 20px;
  --fontS18: 18px;
  --fontS12: 12px;
  --fontS10: 10px;
  height: 100%;
  background-color: var(--BG);
}

.clearfix::after {
  clear: both;
  content: "";
  display: block;
  height: 0;
  line-height: 0;
  visibility: hidden;
  overflow: hidden;
}

.level_vertical_center {
  display: flex;
  justify-content: center;
  align-items: center;
}

.title_font {
  font-size: var(--fontS18);
  color: var(--color333);
}

.sub_title_font {
  font-size: var(--fontS10);
  color: var(--color666);
}

.navigator-hover {
  background: transparent;
}

image {
  width: 100%;
}

.ellipsis {
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  text-overflow: ellipsis;
}

